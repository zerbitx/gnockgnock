{
   "flagAccount": {
      "paths": {
         "/v1/accounts/:userID/flag": {
            "post": {
               "statusCode": 201,
               "responseBodyTemplate": "{\"{{.userID}}\": \"flagged\"}\n"
            }
         }
      }
   },
   "flagAccountNotFound": {
      "paths": {
         "/v1/accounts/:userID/extend": {
            "post": {
               "statusCode": 404,
               "responseBodyTemplate": "No account found for that {{.userID}}\n\n"
            }
         }
      }
   }
}